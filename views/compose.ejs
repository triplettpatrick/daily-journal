<%- include("partials/header"); -%>
        <h1>Compose</h1>
        <form name="Publish" action="/compose" method="POST">
            <div class="form-group">
                <label for="titleInput" class="form-label">Title</label>
                <input class="form-control" type="text" name="postTitle">
            </div>
            <div>
                <label for="bodyInput" class="form-label">Post</label>
                <textarea class="form-control" name="postBody" rows="3"></textarea>
            </div>
            <br>
            <button type="submit" name="button" class="btn btn-primary">Publish</button>
        </form>

<%- include("partials/footer"); -%>